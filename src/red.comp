#version 460 core

layout(local_size_x = 10, local_size_y = 10) in;
layout(rgba32f, binding = 0) uniform image2D outputTexture;

void main() {
    ivec2 outputCoords = ivec2(gl_GlobalInvocationID.xy);
    vec4 outputColor = vec4(1, 0, 0, 1);

    imageStore(outputTexture, outputCoords, outputColor);
}
